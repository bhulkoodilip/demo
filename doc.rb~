print <<EOF
   This is the first way of creating 
   here document i.e. multiple line string.
EOF

print <<"EOF";
   This is the second way of creating 
    here document i.e. multiple line string.
EOF

print <<'EOC'
       echo hi there 
       echo I am here 
EOC

print <<"Das",<<"Na"
       I said Das
Das
       I said Na
Na
print <<Energetic,<<Uandme;
      this is energetic.....
Energetic
      this is uandme
Uandme


print "Hello\n"


for i in 3...10
     print i," hello\n";
end 

